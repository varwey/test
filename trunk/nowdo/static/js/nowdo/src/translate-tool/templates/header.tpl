<% if(!is_authenticated){ %>
    <a class="login-btn" href="javascript:void(0);">登录</a>
    <a class="register-btn" href="javascript:void(0);">注册</a>
<% }else{ %>
    <span class="meta">
        <i class="fa fa-user"></i> <%= nickname %>
        <a href="javascript:void(0);" class="logout-btn"><i class="fa fa-sign-out"></i> 退出登录</a>
        <a href="/" class="logout-btn"><i class="fa fa-reply"></i> 返回首页</a>
    </span>
<% } %>