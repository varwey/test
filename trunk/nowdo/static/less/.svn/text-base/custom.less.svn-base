/********************Translate Tool**********************/
//@ts-page-bg: #e5e4db;
//@ts-body-bg: #f6f4ec;
@ts-page-bg: #ededef;
@ts-body-bg: #FFF;

.task-image-slide, .ts-tool-bar{
  .transition(height 200ms ease);
  height: 0;
}

.task-image-slide{
  padding: 0;
  overflow: hidden;
  position: relative;
  img{
    width: 89px;
    height: 89px;
    float: left;
  }

  &.opened{
//    height: 234px;
    height: 178px;
    .slide-down, .slide-up{
      display: block;
    }
  }

  .slide-down, .slide-up{
    z-index: 5;
    cursor: pointer;
    color: #FFF;
    text-align: center;
    width: 178px;
    position: absolute;
    background: #AAA;
    display: none;
    &:hover{
      background: #CCC;
    }
  }

  .slide-up{
    top: 0;
  }

  .slide-down{
    bottom: 0;
  }

  .slide-left{
    cursor: pointer;
    height: 178px;
    position: absolute;
    width: 25px;
    z-index: 2;

    .fa{
      font-size: 26px;
      margin-top: 69px;
    }
  }
  .slide-left:hover{
      color: #808080;
  }

  .slide-right{
    cursor: pointer;
    height: 178px;
    position: absolute;
    right: 0;
    text-align: right;
    width: 25px;
    z-index: 2;

    .fa{
      font-size: 26px;
      margin-top: 69px;
    }
  }
  .slide-right:hover{
      color: #808080;
  }

  .task-image-slide-wrap{
    overflow: hidden;
    .task-image-slide-inner{
      font-size: 0em;
      position: absolute;
      white-space: nowrap;
      .transition(all 200ms ease);

      a{
        display: inline-block;
        font-size: 14px;
      }
      img{
        width: 178px;
        height: 178px;
      }
    }
  }
}

.ts-tool-bar{
  margin-bottom: 0;
  &.opened{
    height: inherit;
  }
  &>li{
    &.strong a{
      color: #007FE0;
    }
    &.active{
      background-color: @ts-body-bg;
    }
    a{
      display: block;
      padding: 2px 10px;
      color: #7e8282;
      &:hover{
        color: #007FE0;
      }
    }
    a.single-link{
      border-bottom: 1px dashed;
    }
    &:last-child{
      border-bottom-right-radius: 10px !important;
      border-bottom-left-radius: 10px !important;
      &>a{
        border-bottom-right-radius: 10px;
        border-bottom-left-radius: 10px;
      }
    }
  }
}

#ts-container{
  position: relative;
  width: auto;
  max-width: 1248px;

  .ts-tool-panel{
    position: fixed;
    width: 180px;
    margin-top: 40px;
    .panel-xc{
      overflow: hidden;
      .panel-heading{
        cursor: pointer;
      }
    }
  }

  .ts-header{
    height: 40px;
    line-height: 40px;
    text-align: right;
    a{
      margin-left: 10px;
    }
  }

  .ts-workspace{
    background-color: @ts-body-bg;
    text-align: justify;
    line-height: 2;
    padding: 1px 40px 40px;
    margin: 0 0 40px;
    a{
      color: #333;
    }
    #entry-list{
      .row{
        border: 1px solid @ts-body-bg;
        position: relative;
        &:not(:last-child){
          margin-bottom: 10px;
        }
        &:not(.read-mode){
          &:hover{
            border: 1px dashed;
            cursor: pointer;
          }
        }

        &.active{
          border: 1px dashed;
          background-color: #F9FADA;
        }
        .row-wrapper{
          .clear-fix;
          .entry, .result{
            //    text-indent: 2em;
            font-size: 16px;
          }
          .entry{
            a:hover, a.glossary{
              border-bottom: 1px dashed;
            }
          }
          .result{
            position: relative;
            .used-or-not-label{
              left: -5px;
              position: absolute;
              top: -2px;
            }
            .no-result{
              color: red;
            }
          }
        }
      }
    }
    a{
      &.op-btn{
        i{
          display: inherit;
        }
      }
    }
    .translate-editor, .approve-editor{
      z-index: 2;

      position: absolute;
      font-size: 14px;
      .other-results{
        list-style: none;
        padding: 0;
        li{
          padding: 15px;
          border-top: 1px dashed #dddddd;
          .other-result-content{
            .result-heading{
              .clear-fix;
            }
          }
        }

      }

      textarea{
        resize: vertical;
      }
      .panel-xc{
        cursor: default;
        .box-shadow(1px 1px 2px 1px #AAA);
      }
    }

    .translate-editor{
      top: 0;
      .creator-info{
        padding: 0px 15px 15px;
        font-size: @meta-font-size;
      }
      .panel-xc{
        position: relative;
      }
      .panel-footer{
        background: inherit;
        padding: 0px 15px 10px;
        border-top: 0;
      }
      a.close-editor{
        color: #DDDDDD;
        font-size: 18px;
        right: 0;
        top: -12px;
        position: absolute;
        &:hover{
          color: #aaaaaa;
        }
      }
    }
    .approve-editor{
      top: -40px;
      .checked-label{
        padding: 6px 9px;
        i.fa{
          margin-left: 0;
        }
      }
    }
  }
}

