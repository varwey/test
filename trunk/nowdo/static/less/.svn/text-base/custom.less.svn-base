#loading{
  display: none;
  position: fixed;
  left: 50%;
  top: 85px;
  z-index: 99999;
  .loading-content{
    margin-left: -50%;
  }
}

#scroll-top{
  position: fixed;
  right: 100px;
  bottom: 50px;
  font-size: 30px;
  cursor: pointer;
  display: none;
  background: url('../images/to_top.png')  no-repeat;
  width: 48px;
  height: 48px;
  &:hover{
//    color: @ts-body-bg;
    background-position: -100px 0;
  }
}

.ts-alert{
  position: fixed;
  top: 10px;
  z-index: 3;
  left: 50%;
  .alert{
    position: relative;
    left: -50%;
    width: 500px;
  }
}

#sarike-slide-sidebar{
  position: fixed;
  width: 350px;
  right: 0;
  height: 100%;
  background: #fefefe;
  z-index: 999;
  box-shadow: -1px 0 2px 4px rgba(0,0,0, 0.12);
  .slide-heading, .slide-body{
    padding: 15px;
    h1{
      margin-top: 10px;
      font-size: 22px;
    }
  }
  .slide-heading{
    border-bottom: 1px solid #DDDDDD;
  }
  .slide-body{
    .comment-form{
      margin-bottom: 25px;
      .div-input{
        padding: 7px 10px;
      }
    }
    .media-list{
      width: 356px;
      height: 100%;
      padding-right: 20px;
      overflow-y: scroll;
    }
    .media{
      padding: 0px 0 10px;
      border-bottom: 1px dashed #ddd;
    }
    img.media-object{
      width: 30px;
      height: 30px;
      .panel-content-shadow;
    }
    .media-heading{
      &>span{
        .meta;
      }
    }
  }
}
/********************End Translate Tool Style ***********/

/********** Login Page **********/
.login-wrapper{
  background: url("../images/frontend/001.jpg") center no-repeat;
  background-size: cover;
  left: 0px;
  top: 0px;
  position: fixed;
  overflow: hidden;
  margin: 0px;
  padding: 0px;
  height: 100%;
  width: 100%;
  .login-banner{
    padding-top: 100px;
    height: 300px;
    .logo{
      background-image: url("http://127.0.0.1:5050/static/less/../images/bg.png");
      width:414px;
      height:71px;
      background-position: -7px -165px;
    }
    .title{
      .bottom-gap;
      color: #FFF;
      font-size: 24px;
      letter-spacing: 14px;
      margin-top: 10px;
      .text-shadow(0 1px 1px rgba(0,0,0,0.45));
    }
    .buttons{
      text-align: center;
      margin-top: 40px;
      .btn{
        width: 240px;
        padding: 10px 12px;
        &:first-child{
          margin-right: 20px;
        }
      }
    }
  }
  .login-form-wrapper{
//    background-color: #FFF;
    height: 100%;
    .login-container{
      width: 400px;
      color: #FFF;
      a{
        color: #FFF;
      }
      .form-group{
        &.has-feedback .form-control-feedback{
          top: 32px;
        }
      }
    }
  }
}

.pagination{
  .panel-content-shadow();
  float: right;
  margin: 0px 0px 20px;
  &>li{
    &.active>a{
      &,&:hover{
        color: #FFF;
      }
    }
  }
}

.note-editor{
  .note-toolbar{
    background-color: #FCFDFC;
    .btn{
      font-size: 1.2em;
    }
  }
  .note-toolbar, .note-image-popover{
    & .btn-default{
      .box-shadow(none);
      background-color: #ffffff;
      color: #000000;
    }
  }
}

.modal-open{
  overflow: auto;
}

.progress{
  height: 5px;
  .border-radius(0);
}

.popover{
  .popover-hover{
    position: relative;
    background: transparent;
    padding: 0;
    border: 0;
    margin-bottom: 0;
    .box-shadow(none);
  }
  &.bottom{
    .popover-hover{
      margin-top: -50px;
      padding-top: -50px;
    }
  }

  &.user-info-popover{
    height: 109px;
    .popover-hover{
      height: 130px;
    }
  }
  &.glossary-popover{
    .popover-title{
      background-color: #428bca;
      color: #FFF;
      a{
        color: #FFF;
      }
    }
    .popover-content{
      padding: 5px 0;
      ul{
        li{
          padding: 3px 14px;
          &:not(:last-child){
            border-bottom: 1px dashed #ddd;
          }
          &:hover{
            background-color: #DDDDDD;
            cursor: pointer;
          }
        }
      }
    }
  }
  ul{
    list-style: none;
    margin-bottom: 0;
    padding-left: 0px;
    text-align: left;
  }
}

.btn-group{
  &.explore_nav{
    .box-shadow(0 1px 1px rgba(0,0,0,.05));
    border: 1px solid #DDD;
    a.btn{
      padding: 30px 12px;
      border: 0;
      border-bottom: 3px solid #FFF;
      .box-shadow(none);
      &:not(:last-child){
        border-right: 1px solid #DDD;
      }
      &:hover, &.active{
        background: #FFF;
        border-bottom: 3px solid #007FE0;
      }
    }
  }
}

.languages{
  padding-left: 15px;
  margin-bottom: 0;
  a{
    vertical-align: middle;
    .flag{
      background: url("../images/all_countries_flag.gif") no-repeat;
      display: inline-block;
      width: 16px;
      height: 11px;
      &.cn{
        background-position: 0px -1196px;
      }
      &.en{
        background-position: 0px -5954px;
      }

    }
    margin-right: 5px;
  }
}