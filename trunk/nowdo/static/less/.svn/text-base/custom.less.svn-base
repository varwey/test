@import "base.less";

body, html {
  height: 100%;
  margin: 0;
  padding: 0;
  font-family: @font-family;
  font-size: @font-size;
  background-color: @bg-color;
}

.alert-xc{
  position: fixed;
  width: 350px;
  height: 60px;
  top: 85px;
  z-index: 1;
  left: 50%;
  .alert-content{
    position: relative;
    left: -50%;
    height: 100%;
  }
}

.ops{
  a{
    margin-left: 10px;
  }
}

.container{
  width: @content-width;
}

.form-control{
  .border-radius(2px);
}

.div-input{
  &, &:focus{
    border: 1px solid #ddd;
    box-shadow: inset 0px 1px 2px rgba(0,0,0, 0.12);
    padding: 3px 10px;
    min-height: 36px;
  }
}

.overflow-text{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.modal{
  overflow: auto;
  .modal-dialog{
    &.modal-login{
      width: 400px;
    }
    .modal-content{
      .border-radius(2px;);
      .modal-body{
        #register-modal-form, #login-modal-form, #reset-pwd-modal-form{
          span.text-danger{
            display: inline-block;
          }
        }
      }
    }
  }
}

.nav-pills{
  background-color: #FFF;
  .panel-content-shadow;
  .border-radius(10px);
  &>li{
    &>a{
      .border-radius(0);
      &:hover{
        background-color: lighten(@bootstrap-primary-color, 20%);
        color: #FFF;
      }
    }
    &:first-child{
      &>a{
        border-top-left-radius: 10px;
        border-bottom-left-radius: 10px;
      }
    }
//    &:last-child{
//      &>a{
//        border-top-right-radius: 10px;
//        border-bottom-right-radius: 10px;
//      }
//    }
  }
}

//link
a{
  .link-style(#676767, #000);
}

//header
.navbar-xc{
  background-color: @header-bg-color;
  min-height: @header-height;
  margin-bottom: 0;
  .navbar-brand{
    background-image: url("../images/bg.png");
    width:285px;
    height:42px;
    background-position:-10px -112px;
    margin: 19px 0 !important;
//    margin: (@header-height - 40)/2px inherit !important;
  }
  .navbar-btn{
    height: @header-btn-height;
    width: @header-btn-width;
    background-image: url("../images/bg.png");
    background-position:-10px -10px;
    padding: 3px;
    margin: (@header-height - @header-btn-height)/2px 0;
    margin-left: 10px;
    color: #FFF;
  }
  .navbar-nav{
    .navbar-content{
      background-color: transparent;
      border-top: 2px solid @header-bg-color;
      color: #ffffff;
      padding-top:(@header-height - 28)/2px;
      margin-right: 1px;

      .navbar-search{
        background-color: #ffffff;
        border: none;
        border-radius: 15px;
        color: #000000;
        font-size: 12px;
        height: 18px;
        margin: 0px;
        padding: 14px;
        position: relative;
        width: 215px;

        .navbar-search-button{
          border: 0;
          cursor: pointer;
          display: inline-block;
          font-size: 16px;
          height: 30px;
          line-height: 28px;
          position: absolute;
          text-align: center;
          top: 50%;
          left: 0;
          margin: -15px 0 0 0;
          vertical-align: middle;
          width: 30px;
          z-index: 2;
        }

        .navbar-search-input{
          background-color: transparent;
          border: none;
          height: 100%;
          outline: none;
          position: absolute;
          padding-left: 32px;
          padding-right: 14px;
          right: 5px;
          top: 0;
          width: 100%;
        }

        ul.search-dropdown{
          background: white;
          border: 1px solid #aaaaaa;
          border-radius: 5px;
          color: #aaa;
          font-size: 12px;
          left: 0;
          list-style-type: none;
          margin: 0;
          position: absolute;
          padding: 0;
          top: 130%;
          width: 100%;

          li{
            &.divider {
              border-top: 1px solid #aaaaaa;
              padding: 0;
              margin: 0;
            }

            a {
              -moz-transition: all 0.4s;
              -o-transition: all 0.4s;
              -webkit-transition: all 0.4s;
              color: #222;
              display: inline-block;
              padding: 9px 16px 9px 42px;
              position: relative;
              width: 100%;

              span{
                display: inline-block;
                line-height: 10px;
                max-width: 80px;
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
              }

              &:hover{
                background-color: #DDDDDD;
              }

              .fa{
                left: 20px;
                position: absolute;
                top: 12px;
              }
            }
          }
        }
      }
    }

    &>li{
      &, &.open{
        &>a{
          border-top: 2px solid @header-bg-color;
          &:hover{
            border-color: #ffffff;
          }
          color: #ffffff;
          background-color: transparent;
          padding-top:(@header-height - 21)/2px;
          padding-bottom: (@header-height - 21)/2px;
        }
      }
      &.open{
        &>a{
          border-color: #ffffff;
        }
      }
    }
    &.navbar-right{
      &:last-child{
        margin-right: 0;
      }
    }
  }
}

//banner
.banner{
  background-image: url('../images/banner.png');
  height: 400px;
  background-position: center;
  position: relative;
  .pre-view-link{
    position: relative;
    top: 259px;
    left: 85px;
    font-size: 21px;
//    color: #642e10;
    &>a{
      .link-style(@panel-heading-font-color, lighten(@panel-heading-font-color, 10%))
    }
  }
  .join-us{
    position: relative;
    top: 215px;
    left: 93px;
    .joined-count{
      font-size: @meta-font-size;
      margin-bottom: 5px;
      .count{
        font-size: 22px;
      }
    }
    .btn-join-us{
      display: block;
      width:289px;
      height:54px;
      background-image: url("../images/bg.png");
      background-position:-7px -47px;
    }
  }

}

.profile-avatar{
  border: 1px solid #ddd;
  padding: 10px;
}

.page-content{
  padding-top: 50px;

  .img-task-item{
    display: inline-block;
    margin-bottom: 10px;

    img{
      display: block;
      max-width: 100%;
    }
  }
}

.btn{
//  border-color: #FFF;
  .border-radius(1px;);
  .box-shadow(0px 1px 1px rgba(0,0,0,0.15););
  &.btn-default{
//    color: #FFF;
//    background-color: @blue-btn-bg-color;
  }
}

//动态
.trends-navbar{
  -moz-border-radius: 10px;
  -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
  -webkit-border-radius: 10px;
  -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
  background-color: #ffffff;
  border-radius: 10px;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3);
  margin: 0;
  padding: 20px;
  position: relative;
  margin-bottom: 30px;

  h2{
    margin: 0;
    .fa{
      color: #aaaaaa;
    }
  }
}

.trends{
  .trend{
    clear: both;
    margin-bottom: 30px;
    position: relative;

    .left-bar{
      float: left;
      width: 50px;
      .media-object{
        height: 50px;
        width: 50px;
        .box-shadow(1px 2px 1px -1px rgba(0, 0, 0, 0.6));
        .border-radius(10px;);
      }
    }

    .action-str{
      display: block;
      margin-bottom: 15px;
    }
    .to-translate-btn{
      background-image: url('../images/bg.png');
      background-position: -368px -49px;
      margin-top: -42px;
      margin-right: -15px;
      height: 50px;
      width: 50px;
      font-size: 50px;
      text-align: center;
      cursor: pointer;
      float: right;
      display: none;
      &.is_translating{
        background-position: -304px -108px;
      }
      &:hover{
        opacity: 0.5;
        .link-style(#FFF, #DDD);
      }
    }
    &:hover{
      .to-translate-btn{
        display: block;
        .link-style(#FFF, #DDD);
      }
    }
    .trend-content{
      margin-left: 90px;
      background-color: #ffffff;
      .border-radius(10px;);
      .box-shadow(0 1px 2px rgba(0,0,0,0.3););
      position: relative;
      .triangle{
        background: url(../images/arrow.png) no-repeat;
        width: 23px;
        height: 24px;
        position: absolute;
//        border-style: solid;
        z-index: 1;
        left: -20px;
        top: 16px;
        .transform(rotate(10deg););
      }
      .trend-title{
        margin-bottom: 3px;
        margin-left: 10px;
        font-size: @meta-font-size;
        color: #aaa;
      }
      .trend-heading, .trend-body{
        padding-left: 30px;
        padding-right: 15px;
      }
      .trend-heading{
        padding-top: 10px;
        padding-bottom: 10px;
        .featured, .active-task{
          margin-top: -5px;
        }
        h3{
          font-size: @trend-heading-font-size;
          margin:0;
          a{
            .link-style(@trend-heading-font-color, lighten(@trend-heading-font-color, 10%));
          }
        }
      }
      .trend-body{
        line-height: 2;
        padding: 15px 30px;
        word-wrap: break-word;
        overflow: hidden;
        .media-list{
          &.comment-list{
            margin-top: 20px;
            margin-bottom: 0;
            .media{
              font-size: 12px;
              padding: 10px 0;
              margin-top: 0;
              &:not(:last-child){
                border-bottom: 1px dashed #ddd;
              }
              img.media-object{
                width: 20px;
                height: 20px;
                opacity: .7;
                &:hover{
                  opacity: 1;
                }
              }
              .media-body{
                .media-heading{
                  color: @meta-font-color;
                  font-size: @meta-font-size;
                }
              }
            }
          }
        }
        .tags{
          margin: 10px 0 -15px;
        }

        .part-text, .full-text{
          img{
            cursor: pointer;
          }
        }
      }
      .summary-image{
        float: left;
        margin-top: 10px;
        margin-right: 15px;
        position: relative;
        width:200px;

        img{
          max-width: 200px;
          width: auto;
        }

        .total{
          bottom: 5px;
          height: 20px;
          line-height: 20px;
          position: absolute;
          right: 5px;
          width: 20px;
          z-index: 6;

          .totalbg{
            background-color: black;
            border-radius: 2px;
            height: 100%;
            left: 0;
            opacity: 0.5;
            position: absolute;
            top: 0;
            width: 100%;
          }

          .totalnum{
            color: #fff;
            font-size: 10px;
            height: 100%;
            left: 0;
            overflow: hidden;
            position: absolute;
            text-align: center;
            top: 0;
            white-space: nowrap;
            width: 100%;
          }
        }
      }
      .trend-images{
        margin: 10px 0;
        img{
          height: 100px;
          width: 100px;
          margin: 5px;
          cursor: pointer;
        }
      }
      .expand-trend{
        font-size: 12px;
        color: #888;
        .fa{
          font-size: 18px;
          font-weight: bolder;
        }
      }
      .trend-meta{
        a{
          .link-style(#bbb, @header-bg-color);
          margin-right: 5px;
        }
        .meta;
      }

      .languages{
        opacity: 0;
      }

      &:hover{
        .languages{
          opacity: 1;
        }
      }

      .trend-footer{
        padding: 1px 15px 10px 30px;
        .trend-footer-line{
          margin-top: 5px;
        }
        .action-str{
          .trend-meta;
          a{
            margin-right: 1px;
          }
        }
        .ops{
          a{
            margin-left: 10px;
            &.active{
              color: red !important;
            }
          }
          margin-right: 10px;
          .trend-meta;
        }
      }
    }
  }
}

//Panel
.panel{
  .border-radius(@panel-border-radius);
  .list-group{
    .list-group-item{
      border-width: 0;
      .time-label{
        margin-top: 3px;
        float: right;
        .meta;
      }
      &.divider{
        border-top: 1px solid #DDDDDD;
        padding: 0;
        margin: 0 10px;
      }
    }
  }
  .panel-heading {
    border-top-left-radius: @panel-border-radius;
    border-top-right-radius: @panel-border-radius;
  }
  .panel-body{
    line-height: 2;
    .tag{
      padding: 3px 10px;
      margin: 2px 1px;
      display: inline-block;
      &:hover, &.active{
        background-color: @bootstrap-primary-color;
        color: #FFF;
      }
    }
    .card-list{
      margin-top: -20px;
      &.avatar-only{
        margin: -10px;
        .card{
          margin: 1px 0 0 1px;
          width: auto;
          overflow: hidden;
          position: relative;
          .slide-info{
            text-align: center;
            display: block;
            position: absolute;
            width: 50px;
            height: 25px;
            overflow: hidden;
            text-overflow: ellipsis;
            background-color: rgba(0,0,0,0.25);
            top: 100%;
            left: 0;
            .transition(all 500ms ease);
            a{
              color: green;
              font-size: 20px;
              vertical-align: top;
            }
          }
          &:hover{
            .slide-info{
              top: 50%;
            }
          }
        }
      }
      .card{
        float: left;
        width: 185/3px;
        margin-top: 20px;
        .avatar{
          &, & img{
            width: 50px;
            height: 50px;
          }
          margin: 0 auto;
        }
        .card-content{
          text-align: center;
          .title{
            font-size: @meta-font-size;
            margin-top: 5px;
          }
          .count{
            .meta;
          }
        }

        &.card-lg{
          width: 100px;
          .avatar{
            &, & img{
              width: 80px;
              height: 80px;
            }
            margin: 0 auto;
          }
        }
        &.card-horizontal{
          float: left;
          width: 225px;
          .avatar{
            margin: 0;
            float: left;
          }
          .card-content{
            margin-left: 50px;
            padding-left: 10px;
            text-align: left;
            .title{
              margin-top: 0px;
            }
            .count{
              .meta;
            }
          }
          &.card-lg{
            .avatar{
              width: 80px;
              height: 80px;
            }
            .card-content{
              margin-left: 80px;
            }
          }
        }
      }
    }

    ul.recom-follow{
      background-position: -246px 0;
      background-repeat: repeat-y;
      float: left;
      margin: -3px 0 -1px;
      padding: 0;
      width: 100%;
      zoom: 1;
      li{
        &.first{
          border-top: 0;
        }

        background-color: white;
        border-top: 1px solid #dfdfdf;
        display: inline;
        float: left;
        overflow: hidden;
        position: relative;
        width: 100%;
        .mi{
          border-top: 1px solid #fff;
          float: left;
          font-size: 14px;
          overflow: hidden;
          padding: 10px 0;
          line-height: 15px;
          width: 100%;

          .w-img2{
            display: inline;
            float: left;
            height: 30px;
            margin: -2px 10px 0 10px;
            position: relative;
            width: 30px;

            img{
              border: 0;
              display: block;
              height: 30px;
              width: 30px;
            }
          }

          .txt{
            color: #444;
            float: left;
            line-height: 18px;
            margin-right: -120px;
            margin-top: -3px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
            width: 120px;

            .in{
              color: #aaa;
              display: block;
              font-size: 12px;
              line-height: 15px;
            }
          }
        }

        .w-icn2{
          &:hover{
            color:#444;
            cursor: pointer;
          }

          background-position: 0 -830px;
          clear: both;
          color: #aaa;
          display: block;
          font-size: large;
          overflow: hidden;
          position: absolute;
          right: 15px;
          top: 7px;
          width: 18px;
          z-index: 2;
        }
      }
    }

    .panel-footer{
      border-bottom-right-radius: @panel-border-radius;
      border-bottom-left-radius: @panel-border-radius;
    }
  }
  // Panel XC
  &.panel-xc{
    overflow: hidden;
    &.blue-heading{
      .panel-heading{
        background-color: #428bca;
        color: #FFF;
      }
    }
    .panel-heading{
      background-color: @panel-heading-bg-color;
      &.main{
        color: @trend-heading-font-color;
        font-size: @big-panel-heading-font-size;
      }
      .more-link{
        background-image: url("../images/bg.png");
        width:25px;
        height:11px;
        background-position:-91px -6px;
        display: block;
        margin-top: 6px;
      }
    }
    .panel-body{
      .panel-image{
        img{
          width: 160px;
          height: 160px;
          float: left;
        }
      }
      .panel-list{
        margin-left: 175px;
        margin-bottom: 0;
      }
      ul.task-list{
        list-style: none;
        margin-bottom: 0;
        padding-left: 0;
      }
    }
  }

  // Panel Content
  &.panel-content{
    .panel-heading{
      background-color: #ffffff;
      border-bottom: 0;
      h1{
        font-size: @content-heading-font-size;
        color: @trend-heading-font-color;
        .topic-meta{
          font-size: @meta-font-size;
          float: right;
          margin-top: 15px;
        }
      }
      h2{
        margin: 0;
        font-size: 18px;
      }
      h1, h2{
        a.create-url{
          font-size: @meta-font-size;
          margin-top: 5px;
        }
      }
    }
    .panel-footer{
      border-bottom-right-radius: 10px;
      border-bottom-left-radius: 10px;
      background: #FFF;
    }
    .nav-pills{
      border-bottom-left-radius: 0;
      border-bottom-right-radius: 0;
    }
    .nav-pills{
      li:first-child{
        &>a{
          border-bottom-left-radius: 0;
        }
      }
    }
    .panel-body{
      .nav-pills{
        line-height: 20px;
        margin: -15px -15px 0;
      }
      word-break: break-word;
      .group-index-avatar{
        float: left;
        margin-right: 15px;
        margin-top: 6px;
        width: 150px;
        height: 150px;
      }
      line-height: 2;
      label{
        line-height: 20px;
      }
      .archive-task-card-list{
        margin-left: 3.5px;
        .archived-task-card{
          margin:0 5px 5px 0;
//          border: 1px solid #ddd;
          padding: 2px;
          float: left;
          width: @archive-card-width;
          height: @archive-card-height;
          overflow: hidden;
          background: rgba(221,221,221, 0.5);
          position: relative;
          strong{
            padding: 0 5px;
          }
          .task-summary{
            padding: 5px;
            border-top: 1px dashed #ddd;
          }
          .hovered-ops{
            cursor: pointer;
            position: absolute;
            top: 0;
            text-align: center;
            right: @archive-card-width;
            width: @archive-card-width;
            height: @archive-card-height;
            background: rgba(0,0,0,0.7);
//            .transition(all 200ms ease);
            .archive-time{
              color: #FFF;
              padding: 10px 0;
              font-size: 18px;
            }
          }
          &:hover{
            .hovered-ops{
              right: 0px;
            }
          }
          img{
            width: 100%;
          }
        }
      }
      ul.explore-group-list{
        margin-top: -15px;
        margin-bottom: 0;
        .clear-fix;
        .tasks{
          margin-top: 10px;
        }
        .group-item{
          float: left;
          padding: 10px;
          margin-top: 15px;
          border: 1px solid #ddd;
          width: 456px;
          &.left{
            margin-right: 15px;
          }
          .group-avatar{
            &, & img{
              height: 60px;
              width: 60px;
            }
          }
          h3{
            margin: 0 90px 0 70px;
            a{
              font-size: 16px;
            }
            .group-tags{
              display: block;
              margin-top: 5px;
              a.group-tag{
                margin-right: 10px;
                .meta
              }
            }
          }
        }
      }
      div.recommend_tag_task_list{
        .tasks{
          .task-item{
            .border-radius(10px;);
            width: 142px;
            &:not(:last-child){
              margin-right: 15px;
            }
            .tag-label{
              position: absolute;
              bottom: 10px;
              left: 10px;
              .border-radius(5px);
              background: rgba(0,0,0,0.6);
              color: #FFF;
              padding: 0 5px;
            }
            &:hover{
              opacity: 0.6;
            }
          }
        }
      }
      ul.task-list{
        list-style: none;
        margin-bottom: 0;
        padding-left: 0;
        li{
          img{
            height: 20px;
            width: 20px;
          }
          .time-label{
            float: right;
            display: block;
            height: 30px;
            line-height: 30px;
            .meta;
          }
          a, span{
            vertical-align: middle;
          }
        }
      }
      ul.tag_task_list{
        padding-left: 0;
        margin-bottom: 0;
        .clear-fix;
        margin-top: -15px;
        list-style: none;
        li.tag_task{
          &:nth-child(2n + 1){
            float: left;
          }
          &:nth-child(2n){
            float: right;
          }
          margin-top: 15px;
          display: inline-block;
          padding: 10px;
          width: 456px;
          border: 1px solid #ddd;
          h3{
            font-weight: bold;
            font-size: 16px;
            margin: 0 0 5px;
            a{
              display: block;
            }
          }
        }
      }
