          border-top: 2px solid #428bca;
          height: 165px;
          margin: 0;
          padding: 0;
          position: absolute;
          margin-top: 10px;
          width: 10px;

          .placeholder-divider{
            border-left: 2px solid #428bca;
            display: inline-block;
            height: 100%;
            left: 4px;
            margin: 0px;
            padding: 0px;
            position: relative;
          }
        }
      }

      .dropzone{
        border: 1px dashed #dfdfdf;
        color: #aaaaaa;
        margin: 10px;
        overflow: hidden;
        position: relative;
        text-align: center;

        .dropzone-info{
          padding: 100px 0px;
        }

        .dropzone-info-small{
          padding: 15px 0px;
        }

          .dropzone-img{
            font-size: 60px;
          }

          .dropzone-text{
            font-size: 18px;
          }

        #fileupload{
          cursor: pointer;
          direction: ltr;
          font-size: 300px;
          margin: 0;
          opacity: 0;
          position: absolute;
          right: 0;
          top: 0;
        }
      }

      .dropzone-hover{
        background: #aaaaaa;
        color: #ffffff;
      }

      .dropzone-in{
        background-color: grey;
      }

    }
  }
